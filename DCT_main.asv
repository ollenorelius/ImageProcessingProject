function rms = DCT_main(im)

global compRatio_DCT = 0.1;

for i = 1:3
    imPart = im(:,:,i);
    
    trans(:,:,i) = blockproc(imPart, [8 8], @DCT_truncFun);
    
end
trans = uint8(trans);
imagesc(im);
figure()
image(trans);
figure();
error = abs(im-trans);
imagesc(error)
rms = sqrt(mean(mean(mean(error.^2))));

end